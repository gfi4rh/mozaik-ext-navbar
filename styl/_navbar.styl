.navbar
  &__container
    display flex
    height 100%
    padding 2px
  
  &__msg
    flex-grow 1
    background-color #535f66
    margin 0.1em 2vmin
    border-radius 0.5em
    padding 0 0.5em

  &__svg
    display contents
    margin 0 0.5vmin

  &__version
    background-color #27ae60
    margin 0.1em 0.5em
    padding 0 0.5em
    border-radius 0.5em
  
