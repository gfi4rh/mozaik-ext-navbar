.navbar
  &__container
    display flex
    height 100%
    padding 0.4em

    & svg
      cursor pointer
  
  &__msg
    flex-grow 1
    background-color #535f66
    margin 0 2vmin
    border-radius 0.5em
    padding 0 0.5em
    border none
    outline none

  &__version
    background-color #27ae60
    margin 0 0.5em
    padding 0 0.5em
    border-radius 0.5em
    cursor pointer
  
